{
  "name": "node-typescript-starter",
  "version": "0.0.1",
  "description": "Typescript and Express",
  "scripts": {
    "clean": "gulp clean",
    "compile": "gulp compile",
    "watch": "gulp watch",
    "build": "gulp build",
    "deploy": "concurrently --kill-others \"gulp watch\" \"node ./dist/server/bin/www\"",
    "postinstall": "gulp installTypings && gulp build",
    "buildWebpack": "webpack --progress",
    "buildWebpack:prod": "webpack -p --progress",
    "serveWebpack": "webpack-dev-server --host 0.0.0.0 --content-base ./ --history-api-fallback --hot --inline --display-cached --devtool eval"
  },
  "keywords": [
    "Express",
    "Typescript"
  ],
  "author": {
    "name": "Alexandr Lenskiy",
    "email": "alex.lens.dev@gmail.com",
    "url": "https://www.linkedin.com/in/alexandr-lenskiy-66305b9b"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "~2.4.9",
    "@angular/compiler": "~2.4.1",
    "@angular/core": "~2.4.1",
    "@angular/forms": "~2.4.1",
    "@angular/http": "~2.4.1",
    "@angular/platform-browser": "~2.4.1",
    "@angular/platform-browser-dynamic": "~2.4.1",
    "@angular/router": "3.4.9",
    "@angular/upgrade": "~2.4.1",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "dotenv": "^4.0.0",
    "express": "^4.14.0",
    "localize": "^0.4.7",
    "mongoose": "^4.6.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.8",
    "rxjs": "^5.0.1",
    "satellizer": "^0.15.5",
    "systemjs": "0.19.41",
    "zone.js": "^0.7.2",
    "gulp": "^3.9.1"

  },
  "devDependencies": {
    "angular2-template-loader": "0.5.0",
    "concurrently": "^3.0.0",
    "del": "^2.2.2",
    "gulp-cli": "^1.3.0",
    "gulp-concat": "^2.6.0",
    "gulp-nodemon": "^2.2.1",
